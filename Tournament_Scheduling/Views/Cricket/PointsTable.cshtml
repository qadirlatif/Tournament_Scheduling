@model List<Tournament_Scheduling.Models.PointsTable>
@{
    ViewBag.Title = "PointsTable";
}

<h2>PointsTable</h2>
<table class="table table-hover" id="teamstable">
    <thead>
        <tr>
            <th>Team</th>
            <th>Matches</th>
            <th>Wins</th>
            <th>Losses</th>
            <th>Points</th>

        </tr>
    </thead>
    <tbody>
        @foreach(var items in Model)
            {
            <tr>
                <td>@items.TeamName</td>
                <td>@items.Matches</td>
                <td>@items.Wins</td>
                <td>@items.Loss</td>
                <td>@items.Points</td>

            </tr>
            }
    </tbody>
</table>

